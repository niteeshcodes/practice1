   "source": [
    "def prime_check(num):\n",
    "    if num < 2:\n",
    "        return False\n",
    "    for i in range(2,int(num**0.5)+1):\n",
    "        if num % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "num = 21\n",
    "if prime_check(num):\n",
    "    print('prime')\n",
    "else:\n",
    "    print('not prime')"
   ]
